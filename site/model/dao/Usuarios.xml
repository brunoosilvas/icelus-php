<?xml version="1.0" encoding="UTF-8" ?>
<entity name="usuarios" class="\site\model\dao\Usuarios">
	
	<primarykey>
		<column name="id" type="\icelus\orm\type\Integer" sequence="usuarios_id_seq" />
	</primarykey>
	
	<columns>
		<column name="nome" type="\icelus\orm\type\String" />
		<column name="senha" type="\icelus\orm\type\String" />
	</columns>
	
	<foreingkeys>
		<column name="profissao" class="\site\model\dao\Profissoes" nullable="false" />
		<column name="classificacao" class="\site\model\dao\Classificacoes" nullable="false" />
	</foreingkeys>
	
</entity>
